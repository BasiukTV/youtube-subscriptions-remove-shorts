{
    "manifest_version": 3,
    "name": "No Shorts In YouTube Subs",
    "version": "1.0",
    "description": "Removes Shorts from YouTube subscriptions feed.",
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://www.youtube.com/feed/subscriptions*"
          ]
        }
    ]
}
